{
    "name": "sysuwlan",
    "version": "1.0",
    "description": "connect to wlan automatically",
	//"default_locale": "zh_CN",
	"manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
		"default_title": "开启sysuwlan",
		"default_popup": "popup.html"
    },
    "icons": {
        "128": "icon_128.png"
    },
	"background": {
	  "scripts": ["background.js", "sysuwlan.js"]
	},
	"options_page": "options.html",
    "permissions": [
        "tabs",
        "http://10.10.2.22/",
		"http://*/*"
   ]
}

